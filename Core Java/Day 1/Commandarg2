class Commandarg2{
  public static void main(String args[]){
  
   if(args.length<=0)
    {
	  System.out.println("Error: enter parameters..");
	}   
	else
	{
	   int sum=0;
	   for(int i=0;i<args.length;i++)
	   {
	      sum=Integer.parseInt(args[i]);
	   }
	   
	   System.out.println("Sum of command line arguments : "+sum);
	}
  }
}